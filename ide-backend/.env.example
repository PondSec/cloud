NODE_ENV=development
PORT=8080
CORS_ORIGIN=http://localhost:5173,http://127.0.0.1:5173
JWT_SECRET=change-me-dev-ide-jwt
JWT_EXPIRES_IN=8h
APP_ENCRYPTION_KEY=change-me-dev-ide-encryption-key-32bytes
DB_PATH=/data/cloudide.db
WORKSPACES_ROOT=/workspaces
RUNNER_URL=http://runner:8081
RUNNER_WS_URL=ws://runner:8081
WORKSPACE_IMAGE=cloudide-workspace:latest
WORKSPACE_VOLUME=cloudide-workspaces
DEFAULT_CPU_LIMIT=1
DEFAULT_MEM_LIMIT=1024m
DEFAULT_PIDS_LIMIT=256
DEFAULT_ALLOW_EGRESS=true
LOGIN_RATE_LIMIT_MAX=10
LOGIN_RATE_LIMIT_WINDOW_MS=900000
RUNNER_START_RATE_LIMIT_MAX=30
RUNNER_START_RATE_LIMIT_WINDOW_MS=60000
